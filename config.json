{
	"http port": 9000,
	"https port": 9001,
	"proxies": [{
		"from": "http://subdomain.example.com",
		"target": "http://localhost:8080"
	}, {
		"from": "https://secure.example.com",
		"target": "http://localhost:8080",
		"https": {
			"key": "path/to/key",
			"cert": "path/to/cert"
		}
	}],
	"error messages": {
		"not found": "404 not found",
		"proxy error": "An error occurred, please try again"
	}
}